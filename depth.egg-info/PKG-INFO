% Install conda for Linux, for other OS download miniconda at https://docs.conda.io/en/latest/miniconda.html
wget https://repo.anaconda.com/miniconda/Miniconda3-latest-Linux-x86_64.sh
bash Miniconda3-latest-Linux-x86_64.sh -b -p $HOME/miniconda
$HOME/miniconda/bin/conda init bash

% conda create -n AV_ENV python=3.7
conda env create -f conda_env.yml
conda activate AV_ENV

% conda install pytorch==1.8.0 torchvision==0.9.0 torchaudio==0.8.0 cudatoolkit=11.1 -c pytorch -c conda-forge

% pip install pytorch-lightning==1.2.9

pip install mmcv-full==1.3.13 -f https://download.openmmlab.com/mmcv/dist/cu111/torch1.8.0/index.html

pip install -e .

% conda install -c fvcore -c iopath -c conda-forge fvcore iopath
% conda install -c bottler nvidiacub
% conda install pytorch3d -c pytorch3d

% pip install mmsegmentation

% pip install -r requirements.txt 


% pip3 install wldhx.yadisk-direct



export TORCH_HOME=$(pwd) && export PYTHONPATH=.



